{
  "extends": ["config:base"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "lockFileMaintenance": { "enabled": true },
  "packageRules": [
    {
      "matchManagers": ["bundler"],
      "rangeStrategy": "update-lockfile"
    },
    {
      "matchManagers": ["cocoapods"],
      "enabled": true,
      "labels": ["dependencies", "cocoapods"],
      "additionalBranchPrefix": "cocoapods/"
    }
  ]
}
